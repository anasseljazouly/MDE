      <artifactId>org.eclipse.osgi</artifactId>
      <type>osgi-bundle</type>
      <version>3.2.0.v20060601</version>
      <scope>compile</scope>
    </dependency>
    <dependency>
      <groupId>eclipse-sdk-org</groupId>
      <artifactId>org.eclipse.equinox.preferences</artifactId>
      <type>osgi-bundle</type>
      <version>3.2.0.v20060601</version>
