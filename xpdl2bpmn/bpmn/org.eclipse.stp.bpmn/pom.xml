  <modelVersion>4.0.0</modelVersion>
  <artifactId>${parent.groupId}.bpmn</artifactId>
  <packaging>osgi-bundle</packaging>
  <name>org.eclipse.stp.bpmn</name>
  <properties>
    <maven.test.skip>true</maven.test.skip>
  </properties>
  <parent>
    <groupId>org.eclipse.stp.bpmn</groupId>
    <artifactId>intalio-bpmn-gmf-designer-project</artifactId>
    <version>1.0.0-SNAPSHOT</version>
  </parent>
  <build>
    <finalName>${project.artifactId}_${project.version}</finalName>
    <resources>
      <resource>
        <directory>META-INF/..</directory>
        <excludes>
          <exclude>**/.*</exclude>
          <exclude>**/*.jar</exclude>
          <exclude>.settings/**/*</exclude>
          <exclude>pom.xml</exclude>
          <exclude>target/**/*</exclude>
          <exclude>build.properties</exclude>
        </excludes>
      </resource>
      <resource>
        <directory>src</directory>
        <excludes>
          <exclude>**/*.java</exclude>
        </excludes>
      </resource>
    </resources>
    <plugins>
      <plugin>
        <groupId>org.apache.maven.plugins</groupId>
        <artifactId>maven-compiler-plugin</artifactId>
        <configuration>
          <source>1.5</source>
          <target>1.5</target>
        </configuration>
      </plugin>
      <plugin>
        <groupId>com.intalio.mvn.plugins</groupId>
        <artifactId>mvn-eclipse-packager-plugin</artifactId>
        <extensions>true</extensions>
      </plugin>
    </plugins>
    <extensions>
      <extension>
        <groupId>org.apache.maven.wagon</groupId>
        <artifactId>wagon-ssh-external</artifactId>
        <version>1.0-alpha-6</version>
      </extension>
    </extensions>
  </build>
  <dependencies>
    <dependency>
      <groupId>eclipse-sdk-org</groupId>
      <artifactId>org.eclipse.core.runtime</artifactId>
      <type>osgi-bundle</type>
      <version>3.2.0.v20060603</version>
      <scope>compile</scope>
    </dependency>
    <dependency>
      <groupId>eclipse-sdk-org</groupId>
      <artifactId>org.eclipse.core.contenttype</artifactId>
      <type>osgi-bundle</type>
      <version>3.2.0.v20060603</version>
      <scope>compile</scope>
    </dependency>
    <dependency>
      <groupId>eclipse-sdk-org</groupId>
      <artifactId>org.eclipse.equinox.registry</artifactId>
      <type>osgi-bundle</type>
      <version>3.2.0.v20060601</version>
      <scope>compile</scope>
    </dependency>
    <dependency>
      <groupId>eclipse-sdk-org</groupId>
      <artifactId>org.eclipse.equinox.common</artifactId>
      <type>osgi-bundle</type>
      <version>3.2.0.v20060603</version>
      <scope>compile</scope>
    </dependency>
    <dependency>
